{
  "grammars": [
    {
      "name": "twig",
      "camelcase": "Twig",
      "scope": "twig",
      "path": ".",
      "file-types": [
        "twig",
        "html.twig"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injections": "queries/injections.scm",
      "injection-regex": "twig"
    }
  ],
  "metadata": {
    "version": "0.4.0",
    "license": "Mozilla Public License 2.0",
    "description": "Twig grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-twig"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false
  }
}
